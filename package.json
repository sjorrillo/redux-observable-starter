{
  "name": "redux-observable-starter",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "bnr build:dev",
    "build": "bnr build:prod",
    "lint": "npm run lint-files -- src --ext .js,.ts,.tsx",
    "lint:fix": "npm run lint -- --fix ",
    "lint-files": "eslint --cache --cache-location='.cache/' -f friendly",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "test": "FORCE_COLOR=true TZ=UTC node --max-old-space-size=4096 node_modules/.bin/jest --notify --logHeapUsage --detectOpenHandles --config jest.config.js"
  },
  "betterScripts": {
    "build:dev": {
      "command": "parcel resources/public/index.html --out-dir build/debug",
      "env": {
        "NODE_ENV": "development",
        "PORT": 3500
      }
    },
    "build:prod": {
      "command": "parcel build resources/public/index.html --experimental-scope-hoisting --detailed-report --out-dir build/release --public-url ./",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "devDependencies": {
    "@babel/core": "^7.8.6",
    "@babel/plugin-proposal-export-default-from": "^7.8.3",
    "@babel/plugin-proposal-export-namespace-from": "^7.8.3",
    "@babel/preset-env": "^7.8.6",
    "@babel/preset-react": "^7.8.3",
    "@types/jest": "^25.1.3",
    "@types/lodash": "^4.14.149",
    "@types/node": "^13.7.6",
    "@types/react": "^16.8.6",
    "@types/react-redux": "^7.1.7",
    "@types/redux-actions": "^2.6.1",
    "@types/yup": "^0.26.32",
    "better-npm-run": "^0.1.1",
    "eslint": "^6.8.0",
    "eslint-config-universe": "^2.3.0",
    "eslint-formatter-friendly": "^7.0.0",
    "eslint-plugin-react-hooks": "^2.5.0",
    "jest": "^25.2.3",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-bundle-visualiser": "^1.2.0",
    "parcel-plugin-clean-dist": "^0.0.6",
    "parcel-plugin-static-files-copy": "^2.3.1",
    "prettier": "1.19.1",
    "typescript": "^3.8.2"
  },
  "dependencies": {
    "@material-ui/core": "^4.9.9",
    "@material-ui/icons": "^4.9.1",
    "classnames": "^2.2.6",
    "connected-react-router": "^6.7.0",
    "lodash": "^4.17.15",
    "query-string": "^6.11.1",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "react-hook-form": "^5.3.1",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "redux": "^2.0.0 || ^3.0.0 || ^4.0.0-0",
    "redux-actions": "^2.6.5",
    "redux-observable": "^1.2.0",
    "rxjs": "^6.5.4",
    "yup": "^0.28.3"
  },
  "staticFiles": {
    "staticPath": [
      "resources/public"
    ]
  }
}
